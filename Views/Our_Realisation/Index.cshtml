@model IEnumerable<Cotrageco.Models.Our_Realisation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Realisation_Captions)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Realisation_Image)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.Raw(item.Realisation_Captions.Realisation_CaptionId)
                </td>
                <td>
                    @Html.Raw(item.Realisation_Image)
                </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.Our_RealisationId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Our_RealisationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Our_RealisationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
